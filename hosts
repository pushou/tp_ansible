debian5 ansible_host=127.0.0.1 ansible_ssh_port=2225 ansible_user=root 
debian6 ansible_host=127.0.0.1 ansible_ssh_port=2226 ansible_user=root 
debian7 ansible_host=127.0.0.1 ansible_ssh_port=2227 ansible_user=root 
debian8 ansible_host=127.0.0.1 ansible_ssh_port=2228 ansible_user=root 
debian9 ansible_host=127.0.0.1 ansible_ssh_port=2229 ansible_user=root 


